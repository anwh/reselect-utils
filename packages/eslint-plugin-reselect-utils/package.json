  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "common-tags": "^1.8.0",
    "re-reselect": "^4.0.0",
    "reselect": "^4.0.0",
    "reselect-utils": "2.0.0-beta.14"
  }
