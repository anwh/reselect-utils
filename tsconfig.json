{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2018", "dom"],
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "isolatedModules": true,
    "jsx": "react",
    "noUnusedLocals": true
  }
}
