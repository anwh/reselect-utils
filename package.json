{
  "name": "reselect-utils",
  "version": "2.0.0-alpha.4",
  "description": "A collection of helpers and utilities for selectors",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git+https://github.com/sgrishchenko/reselect-utils.git",
  "author": "Sergei Grishchenko",
  "license": "MIT",
  "files": [
    "dist/",
    "es/",
    "lib/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf lib es dist coverage",
    "build:cjs": "tsc -p tsconfig.build.json --outDir lib -d",
    "build:es": "tsc -p tsconfig.build.json --outDir es -m es2015",
    "build:umd": "rollup -c rollup.config.ts",
    "build": "yarn clean && yarn build:cjs && yarn build:es && yarn build:umd",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "format": "prettier --write **/*.{js,ts,tsx,json,md}",
    "compile": "tsc --noEmit",
    "storybook": "start-storybook -p 9001",
    "postversion": "git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn compile && yarn test && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "yarn lint"
    ],
    "**/*.{js,ts,tsx,json,md}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@types/cytoscape": "^3.4.3",
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__react": "^4.0.2",
    "@types/webpack": "^4.4.35",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.5",
    "cytoscape": "^3.8.1",
    "cytoscape-dagre": "^2.2.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "re-reselect": "^3.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "reselect": "^4.0.0",
    "reselect-tools": "^0.0.7",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.7",
    "rollup-plugin-typescript2": "^0.22.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  }
}
