{
  "name": "reselect-utils",
  "version": "2.0.0-alpha.4",
  "description": "A collection of helpers and utilities for selectors",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git+https://github.com/sgrishchenko/reselect-utils.git",
  "author": "Sergei Grishchenko",
  "license": "MIT",
  "files": [
    "dist/",
    "es/",
    "lib/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf lib es dist coverage",
    "build:cjs": "tsc -p tsconfig.build.json --outDir lib -d",
    "build:es": "tsc -p tsconfig.build.json --outDir es -m es2015",
    "build:umd": "rollup -c rollup.config.ts",
    "build": "yarn clean && yarn build:cjs && yarn build:es && yarn build:umd",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "format": "prettier --write **/*.{js,ts,tsx,json,md}",
    "compile": "tsc --noEmit",
    "storybook": "start-storybook -p 9001",
    "postversion": "git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn compile && yarn test && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "yarn lint"
    ],
    "**/*.{js,ts,tsx,json,md}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/cytoscape": "^3.8.5",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.21",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-redux": "^7.1.5",
    "@types/storybook__react": "^4.0.2",
    "@types/webpack": "^4.39.8",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.7",
    "cytoscape": "^3.11.0",
    "cytoscape-dagre": "^2.2.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "re-reselect": "^3.3.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "reselect": "^4.0.0",
    "reselect-tools": "^0.0.7",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.3",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
