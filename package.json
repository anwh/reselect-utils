{
  "name": "reselect-utils",
  "version": "1.0.9",
  "description": "A collection of helpers and utilities for selectors",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git+https://github.com/sgrishchenko/reselect-utils.git",
  "author": "Sergei Grishchenko",
  "license": "MIT",
  "files": [
    "dist/",
    "es/",
    "lib/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf lib es dist coverage",
    "build:commonjs": "tsc -p tsconfig.build.json --outDir lib -d",
    "build:umd": "rollup -c rollup.config.ts",
    "build:es": "tsc -m es2015 -p tsconfig.build.json --outDir es",
    "build": "yarn clean && yarn build:commonjs && yarn build:umd && yarn build:es",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "format": "prettier --write **/*.{js,ts,tsx,json,md}",
    "compile": "tsc --noEmit",
    "storybook": "start-storybook -p 9001",
    "postversion": "git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "yarn compile",
      "yarn lint",
      "yarn test"
    ],
    "**/*.{js,ts,tsx,json,md}": [
      "yarn format",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@types/cytoscape": "^3.4.0",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-redux": "^7.0.6",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@types/webpack": "^4.4.27",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "cytoscape": "^3.5.2",
    "cytoscape-dagre": "^2.2.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "re-reselect": "^3.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "reselect-tools": "^0.0.7",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.3"
  }
}
