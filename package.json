{
  "name": "reselect-utils",
  "version": "1.0.7",
  "description": "A collection of helpers and utilities for selectors",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git+https://github.com/sgrishchenko/reselect-utils.git",
  "author": "Sergei Grishchenko",
  "license": "MIT",
  "files": [
    "dist",
    "es",
    "lib",
    "!__data__",
    "!__tests__",
    "!__stories__"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf lib es dist coverage",
    "build:commonjs": "tsc -p tsconfig.build.json --outDir lib -d",
    "build:umd": "rollup -c rollup.config.ts",
    "build:es": "tsc -m es2015 -p tsconfig.build.json --outDir es",
    "build": "yarn clean && yarn build:commonjs && yarn build:umd && yarn build:es",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.tsx . && tslint -p tsconfig.json",
    "format": "prettier --write **/*.{js,ts,tsx,json,md}",
    "storybook": "start-storybook -p 9001"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/cytoscape": "^3.2.9",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.0",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "@types/webpack": "^4.4.22",
    "@typescript-eslint/eslint-plugin": "^1.0.0",
    "@typescript-eslint/parser": "^1.0.0",
    "babel-loader": "^8.0.5",
    "cytoscape": "^3.3.1",
    "cytoscape-dagre": "^2.2.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "re-reselect": "^3.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "reselect-tools": "^0.0.7",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2"
  }
}
